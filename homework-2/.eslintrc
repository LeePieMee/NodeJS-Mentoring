{
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true,
        "es6": true,
        "jest": true
    },
    "extends": ["standard", "plugin:@typescript-eslint/recommended", "prettier/@typescript-eslint", "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "semi": [2, "always"]
    }
}
